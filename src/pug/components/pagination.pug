.pagination
  a.pagination__nav.pagination__nav--prev
    +svg('pagination__nav-icon', 'icon-pag-arrow', 7, 12)
  ul.pagination__list
    li.pagination__item
      a.pagination__link 1
    li.pagination__item
      a.pagination__link(href='#') 2
    li.pagination__item
      a.pagination__link(href='#') 3
    li.pagination__item
      a.pagination__link(href='#') 4
    li.pagination__item
      a.pagination__link(href='#') 5
  a.pagination__nav.pagination__nav--next(href='#')
    +svg('pagination__nav-icon', 'icon-pag-arrow', 7, 12)
